#include <stdio.h>
#include "platform.h"
#include "xil_printf.h"
#include "led_axi.h"
#include "xparameters.h"
#include <stdint.h>
#include "xil_io.h"


int main(void) {
	uint32_t *LEDS = XPAR_LED_AXI_0_LED_IO_AXI_BASEADDR;


	init_platform();

	*LEDS = 0xFF;
	Xil_Out32(XPAR_LED_AXI_0_LED_IO_AXI_BASEADDR, 0xFF);

	while(1);

	cleanup_platform();
	return 0;
}
